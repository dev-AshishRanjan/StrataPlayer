{
  "name": "strataplayer",
  "version": "1.2.19",
  "description": "A modern, framework-agnostic media engine for the web. Supports HLS, DASH, MPEG-TS, and WebTorrent with a plugin-based architecture and React integration.",
  "type": "module",
  "main": "./dist/strataplayer.cjs.js",
  "module": "./dist/strataplayer.es.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-AshishRanjan/StrataPlayer.git"
  },
  "keywords": [
    "video-player",
    "html5-video",
    "hls",
    "dash",
    "mpegts",
    "webtorrent",
    "streaming",
    "react",
    "media-player",
    "p2p"
  ],
  "author": "Kumar Ashish Ranjan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dev-AshishRanjan/StrataPlayer/issues"
  },
  "homepage": "https://dev-ashishranjan.github.io/StrataPlayer/",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/strataplayer.es.js",
      "require": "./dist/strataplayer.cjs.js",
      "default": "./dist/strataplayer.es.js"
    },
    "./hls": {
      "types": "./dist/hls.d.ts",
      "import": "./dist/hls.es.js",
      "require": "./dist/hls.cjs.js",
      "default": "./dist/hls.es.js"
    },
    "./dash": {
      "types": "./dist/dash.d.ts",
      "import": "./dist/dash.es.js",
      "require": "./dist/dash.cjs.js",
      "default": "./dist/dash.es.js"
    },
    "./mpegts": {
      "types": "./dist/mpegts.d.ts",
      "import": "./dist/mpegts.es.js",
      "require": "./dist/mpegts.cjs.js",
      "default": "./dist/mpegts.es.js"
    },
    "./webtorrent": {
      "types": "./dist/webtorrent.d.ts",
      "import": "./dist/webtorrent.es.js",
      "require": "./dist/webtorrent.cjs.js",
      "default": "./dist/webtorrent.es.js"
    },
    "./style.css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "dash": [
        "./dist/dash.d.ts"
      ],
      "hls": [
        "./dist/hls.d.ts"
      ],
      "mpegts": [
        "./dist/mpegts.d.ts"
      ],
      "webtorrent": [
        "./dist/webtorrent.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode lib && vite build --mode demo",
    "build:lib": "tsc && vite build --mode lib",
    "build:site": "tsc && vite build --mode demo",
    "preview": "vite preview",
    "preview:site": "vite preview --outDir dist-site"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "hls.js": ">=1.0.0",
    "dashjs": ">=4.0.0",
    "mpegts.js": ">=1.0.0",
    "webtorrent": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "hls.js": {
      "optional": true
    },
    "dashjs": {
      "optional": true
    },
    "mpegts.js": {
      "optional": true
    },
    "webtorrent": {
      "optional": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/webtorrent": "^0.109.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "hls.js": "^1.5.1",
    "dashjs": "^4.7.3",
    "mpegts.js": "^1.7.3",
    "webtorrent": "^2.1.3",
    "readable-stream": "^4.4.0",
    "postcss": "^8.4.32",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-node-polyfills": "^0.21.0",
    "wouter": "^3.0.0"
  }
}
